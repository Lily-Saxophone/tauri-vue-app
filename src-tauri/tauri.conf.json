{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "tauri-vue-app",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "window": {
        "all": true
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "縦書きエディタ",
        "width": 1091,
        "height": 788
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}